<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>june30</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>SisServlet</display-name>
    <servlet-name>SisServlet</servlet-name>
    <servlet-class>com.sis.SisServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SisServlet</servlet-name>
    <url-pattern>/sis</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.sis.ProjectNotficationListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.sis.MyRequestListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>Logout</display-name>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.sis.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  
  
   <listener>
    <listener-class>com.sis.MySessionListener</listener-class>
  </listener>
  
  <servlet>
    <description></description>
    <display-name>TotalLogedInUser</display-name>
    <servlet-name>TotalLogedInUser</servlet-name>
    <servlet-class>com.sis.TotalLogedInUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TotalLogedInUser</servlet-name>
    <url-pattern>/totalLogedInUser</url-pattern>
  </servlet-mapping>
  
</web-app>